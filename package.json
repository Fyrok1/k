{
  "name": "k",
  "version": "0.0.1",
  "description": "k",
  "main": "dist/k/index.js",
  "scripts": {
    "tsc": "tsc",
    "start": "tsc && npm run scss-build && node --inspect=0.0.0.0:9300 -r dotenv/config dist/k/index.js",
    "serve": "concurrently 'npm:scss' 'npm:node'",
    "serve-with-apps": "concurrently 'npm:scss' 'npm:node' npm:apps",
    "apps": "concurrently 'npm:app'",
    "app": "cd apps/app && npm run serve",
    "node": "ts-node-dev --inspect=0.0.0.0:9300 -r dotenv/config --respawn --transpile-only ./src/k/index.ts",
    "scss": "node-sass-chokidar ./scss -o ./public --watch -r --output-style compressed",
    "scss-build": "node-sass-chokidar ./scss -o ./public -r --output-style compressed",
    "lint": "eslint . --ext .ts"
  },
